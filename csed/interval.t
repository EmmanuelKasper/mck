# 
# test \{n,m\} numbers greater than 127
# 

=IN 0123456789
=OUT
50
65
00
75
99
=SED
s/./_&/g
x
g;y/_/0/;G;y/_/1/;G;y/_/2/;G;y/_/3/;G;y/_/4/
G;y/_/5/;G;y/_/6/;G;y/_/7/;G;y/_/8/;G;y/_/9/
s/\n//g
s/.*/&&&/
h;s/.\{100\}\(..\).*/\1/p
g;s/.\{130\}\(..\).*/\1/p
g;s/.\{200\}\(..\).*/\1/p
g;s/.\{1,350\}\(..\).*/\1/p
g;s/.\{350,\}\(..\).*/\1/
.

#
# test s/a/b/N
#

=IN 0123456789
=OUT
50
60
56
57
00
99
=SED
s/./_&/g
x
g;y/_/0/;G;y/_/1/;G;y/_/2/;G;y/_/3/;G;y/_/4/
G;y/_/5/;G;y/_/6/;G;y/_/7/;G;y/_/8/;G;y/_/9/
# 200 bytes in pattern space
s/\n//g
s/.*/&&&&&&&&&&/
# times ten: now 2000 bytes in pattern space
h
g; s/./&_/100 
s/^[^a_]*_\(..\).*/\1/
p
g; s/./&_/120 
s/^[^a_]*_\(..\).*/\1/
p
g; s/./&_/512 
s/^[^a_]*_\(..\).*/\1/
p
g; s/./&_/514 
s/^[^a_]*_\(..\).*/\1/
p
g; s/./&_/1000 
s/^[^a_]*_\(..\).*/\1/
p
g; s/./&_/1998 
s/^[^a_]*_\(..\).*/\1/
.

